{
  "name": "sjsitapp-agent",
  "version": "0.2.1",
  "main": "app.js",
  "description": "An application for the San Jose Sharks Ice Team.",
  "repository": "https://github.com/mattcarlotta/SJSITApp-Agent",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "build": "rimraf build && babel ./ -d build --no-comments --ignore node_modules,**/__tests__,coverage,setupTests.js,**/__mocks__",
    "dev": "NODE_ENV=development nodemon --exec babel-node ./app.js",
    "drop": "NODE_ENV=test DROPDB=true babel-node ./utils/teardownDB.js",
    "seed": "NODE_ENV=test SEEDDB=true babel-node ./utils/seedDB.js",
    "start": "NODE_ENV=production node ./build/app.js",
    "test": "NODE_ENV=test DROPDB=true jest --config ./jest.json --passWithNoTests",
    "test:cov": "NODE_ENV=test DROPDB=true jest --config ./jest.json --coverage",
    "test:watch": "NODE_ENV=test jest --config ./jest.json --passWithNoTests --watchAll"
  },
  "dependencies": {
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@sendgrid/mail": "^6.4.0",
    "axios": "^0.19.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chalk": "^4.0.0",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.27",
    "mongoose": "^5.7.13",
    "node-schedule": "^1.3.2",
    "nodemon": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "alias-dirs": "^0.0.11",
    "axios-mock-adapter": "^1.17.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0"
  }
}
