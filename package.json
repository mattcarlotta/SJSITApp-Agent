{
  "name": "sjsitapp-agent",
  "version": "2.0.5",
  "main": "index.js",
  "description": "An automated microservice to help manage the San Jose Sharks Ice Team scheduling.",
  "repository": "https://github.com/mattcarlotta/SJSITApp-Agent",
  "author": "Matt Carlotta",
  "license": "MIT",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "build": "rimraf build && tsc --p ./tsconfig.prod.json",
    "lint": "eslint -c ./.eslintrc --ignore-path ./.eslintignore ./ --ext .ts",
    "start": "LOAD_CONFIG=prod node -r ./prod-paths.js -r @noshot/env build/index.js",
    "drop": "LOAD_CONFIG=teardown ts-node --project ./tsconfig.json -r tsconfig-paths/register ./database/teardownDB/index.ts",
    "seed": "LOAD_CONFIG=seed ts-node --project ./tsconfig.json -r tsconfig-paths/register ./database/seedDB/index.ts",
    "dev": "nodemon --config ./nodemon.json",
    "stage": "LOAD_CONFIG=stage node -r ./prod-paths.js -r @noshot/env build/index.js",
    "test": "LOAD_CONFIG=test jest --config ./jest.json --passWithNoTests",
    "test:cov": "LOAD_CONFIG=test jest --config ./jest.json --coverage",
    "test:failed": "LOAD_CONFIG=test jest --config ./jest.json --watch --onlyFailures",
    "test:watch": "LOAD_CONFIG=test jest --config ./jest.json --watch",
    "tsc": "tsc --p ./tsconfig.json"
  },
  "dependencies": {
    "@noshot/env": "^1.0.1",
    "@sendgrid/mail": "^7.4.4",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "cheerio": "^1.0.0-rc.9",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "moment-timezone": "^0.5.33",
    "mongoose": "^5.12.12",
    "node-schedule": "^2.0.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.35",
    "@types/jest": "^26.0.23",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isempty": "^4.4.6",
    "@types/mongoose": "^5.11.93",
    "@types/node": "^15.12.0",
    "@types/node-schedule": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "axios-mock-adapter": "^1.19.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^27.0.2"
  }
}
