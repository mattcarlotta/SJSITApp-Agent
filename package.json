{
  "name": "sjsitapp-agent",
  "version": "2.0.0",
  "main": "index.js",
  "description": "An automated microservice to help manage the San Jose Sharks Ice Team scheduling.",
  "repository": "https://github.com/mattcarlotta/SJSITApp-Agent",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "build": "rimraf build && tsc --p ./tsconfig.prod.json",
    "lint": "eslint -c ./.eslintrc --ignore-path ./.eslintignore ./ --ext .ts",
    "start": "ENV_DEBUG=true ENV_LOAD=.env.prod node -r ./prod-paths.js -r snackables build/index.js",
    "drop": "ENV_DEBUG=true ENV_LOAD=.env.teardown,.env.test ts-node --project ./tsconfig.json -r tsconfig-paths/register ./database/teardownDB/index.ts",
    "seed": "ENV_DEBUG=true ENV_LOAD=.env.seed,.env.test ts-node --project ./tsconfig.json -r tsconfig-paths/register ./database/seedDB/index.ts",
    "dev": "nodemon --config ./nodemon.json",
    "stage": "ENV_DEBUG=true ENV_LOAD=.env.staging node -r ./prod-paths.js -r snackables build/index.js",
    "test": "ENV_DEBUG=true ENV_LOAD=.env.test jest --config ./jest.json --passWithNoTests",
    "test:cov": "ENV_DEBUG=true ENV_LOAD=.env.test jest --config ./jest.json --coverage",
    "test:failed": "ENV_DEBUG=true ENV_LOAD=.env.test jest --config ./jest.json --watch --onlyFailures",
    "test:watch": "ENV_DEBUG=true ENV_LOAD=.env.test jest --config ./jest.json --watch",
    "tsc": "tsc --p ./tsconfig.json"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "cheerio": "^1.0.0-rc.6",
    "dotenv": "^8.2.0",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "moment-timezone": "^0.5.33",
    "mongoose": "^5.12.7",
    "node-schedule": "^2.0.0",
    "nodemon": "^2.0.7",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/jest": "^26.0.23",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isempty": "^4.4.6",
    "@types/mongoose": "^5.10.5",
    "@types/node-schedule": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "alias-dirs": "^0.0.11",
    "axios-mock-adapter": "^1.19.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^26.5.5"
  }
}
